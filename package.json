{
	"name": "node-starter",
	"version": "0.0.0",
	"private": "true",
	"license": "MIT",
	"author": "osdevisnot <osdevisnot@gmail.com>",
	"type": "module",
	"scripts": {
		"clean": "git clean -fdX",
		"start": "node ./build.js start",
		"build": "node ./build.js build",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "xo --fix",
		"format": "prettier --write . "
	},
	"xo": {
		"prettier": true
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 120,
		"singleQuote": true,
		"useTabs": true
	},
	"devDependencies": {
		"c8": "^7.12.0",
		"esbuild": "^0.14.53",
		"prettier": "^2.7.1",
		"vitest": "^0.20.3",
		"xo": "^0.51.0"
	}
}
